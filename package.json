{
  "name": "scorder",
  "version": "0.0.3",
  "private": true,
  "productName": "Scorder",
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {},
  "resolutions": {
    "styled-components": "^5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-electron": "electron .",
    "package:linux": "electron-builder build --linux -c.extraMetadata.main=build/electron.js --publish never",
    "package:win": "electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never",
    "package:mac": "electron-builder build --mac -c.extraMetadata.main=build/electron.js --publish never",
    "publish:linux": "electron-builder build --linux --publish always",
    "publish:win": "electron-builder build --win --publish always",
    "publish:mac": "electron-builder build --mac --publish always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.65",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "electron": "^21.1.0",
    "electron-builder": "^23.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4",
    "react-icons": "^4.6.0"
  },
  "build": {
    "mac": {
      "publish": {
        "provider": "github",
        "owner": "anusikh",
        "repo": "scorder-electron",
        "private": true,
        "token": ""
      },
      "icon": "build/scorder.png"
    },
    "win": {
      "publish": {
        "provider": "github",
        "owner": "anusikh",
        "repo": "scorder-electron",
        "private": true,
        "token": ""
      },
      "icon": "build/scorder.png"
    },
    "linux": {
      "publish": {
        "provider": "github",
        "owner": "anusikh",
        "repo": "scorder-electron",
        "private": true,
        "token": ""
      },
      "icon": "build/scorder.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "private": true,
      "owner": "anusikh"
    }
  }
}
